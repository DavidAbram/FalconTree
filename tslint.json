{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "max-line-length": {
          "options": [120]
      },
      "no-console": {
          "severity": "warning",
          "options": [
              "debug",
              "info",
              "log",
              "time",
              "timeEnd",
              "trace"
          ]
      },
      "semicolon": [true, "always"]
  },
  "rulesDirectory": []
}
